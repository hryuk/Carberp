void ClearHTMLInjectList(PList List)
{
	// Функция рчищает список содержащий элементы типа THTMLInject
	if (List == NULL)
		return;
	List::SetFreeItemMehod(List, (TFreeItemMethod)FreeHTMLInject);
    List::Clear(List);
}

