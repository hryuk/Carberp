                    ENLINK *lpLink=(ENLINK*)lParam;
                    if (lpLink->msg == WM_LBUTTONUP)
                    {
						pSendMessageA(lpLink->nmhdr.hwndFrom,EM_EXSETSEL,0,(LPARAM)&lpLink->chrg);
                        TCHAR szBuf[260];
