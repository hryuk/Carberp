	OutMessage("java_patcher", bres? ("ShellExecuteEx(1) STOP - 1"): ("ShellExecuteEx(1) STOP - 0"));
	MemFree(cmd);

	bres = (BOOL)pSetCurrentDirectoryA(JREPath);

