	//pMoveFileExA("\\lib\\rt2.jar","\\lib\\rt.jar",MOVEFILE_REPLACE_EXISTING | MOVEFILE_DELAY_UNTIL_REBOOT);

	CHAR* PathToFlagFile = (CHAR*)MemAlloc(MAX_PATH);
